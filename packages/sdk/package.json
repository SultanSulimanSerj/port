{
  "name": "@saas/sdk",
  "version": "0.0.1",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "generate": "openapi-generator-cli generate -i http://localhost:3001/api-json -g typescript-axios -o ./src/generated --additional-properties=supportsES6=true,npmName=@saas/sdk,modelPropertyNaming=camelCase"
  },
  "dependencies": {
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@saas/config": "workspace:*",
    "@openapitools/openapi-generator-cli": "^2.9.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.2"
  }
}