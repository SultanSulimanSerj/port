{
  "name": "@saas-portal/sdk",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate:sdk": "openapi-generator-cli generate -i ../../apps/api/openapi.json -g typescript-axios -o ./src/generated --additional-properties=npmName=@saas-portal/sdk,supportsES6=true,withInterfaces=true,useSingleRequestParameter=true",
    "lint": "eslint src/",
    "clean": "rm -rf dist src/generated"
  },
  "dependencies": {
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@saas-portal/config": "workspace:*",
    "@types/node": "^20.5.0",
    "typescript": "^5.1.6"
  }
}